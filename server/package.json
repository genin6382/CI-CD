{
  "name": "sample-nodejs-cicd-app",
  "version": "1.0.0",
  "description": "Sample Node.js app with CI/CD pipeline, rate limiting, and PostgreSQL",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "nodemon src/app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "node src/database/migrate.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "nodejs",
    "express",
    "postgresql",
    "ci/cd",
    "rate-limiting"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "pg": "^8.11.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "eslint": "^8.41.0",
    "jest": "^29.5.0",
    "nodemon": "^3.1.10",
    "supertest": "^6.3.3"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/database/migrate.js"
    ]
  }
}
